{
    "name": "rakuten-ws/rws-php-sdk",
    "type": "library",
    "homepage": "https://webservice.rakuten.co.jp",
    "description": "The SDK of Rakuten Web Service",
    "keywords": ["WebAPI", "Client", "SDK"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shogo Kawahara",
            "email": "shogo.a.kawahara@mail.rakuten.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.5"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Rakuten\\WebService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rakuten\\WebService\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
